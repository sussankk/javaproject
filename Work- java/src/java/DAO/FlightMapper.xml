<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="DAO.FlightMapper">
  <resultMap id="BaseResultMap" type="POJO.Flight">
    <result column="FlightCode" jdbcType="INTEGER" property="flightcode" />
    <result column="FlightNum" jdbcType="VARCHAR" property="flightnum" />
    <result column="Price" jdbcType="VARCHAR" property="price" />
  </resultMap>
  <insert id="insert" parameterType="POJO.Flight">
    insert into Flight (FlightCode, FlightNum, Price
      )
    values (#{flightcode,jdbcType=INTEGER}, #{flightnum,jdbcType=VARCHAR}, #{price,jdbcType=VARCHAR}
      )
  </insert>
  <insert id="insertSelective" parameterType="POJO.Flight">
    insert into Flight
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="flightcode != null">
        FlightCode,
      </if>
      <if test="flightnum != null">
        FlightNum,
      </if>
      <if test="price != null">
        Price,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="flightcode != null">
        #{flightcode,jdbcType=INTEGER},
      </if>
      <if test="flightnum != null">
        #{flightnum,jdbcType=VARCHAR},
      </if>
      <if test="price != null">
        #{price,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>

  <select id="getFlight" resultType="POJO.Flight">
    select * from Flight;
  </select>
</mapper>